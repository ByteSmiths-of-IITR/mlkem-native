ifndef MLKEM_K
       $(warning MLKEM_K not set -- defaulting to MLKEM_K=3)
endif

MLKEM_K ?= 3

ifeq ($(MLKEM_K),2)
     MLKEM_NAMESPACE = pqcrystals_mlkem512_ref_
else ifeq ($(MLKEM_K),3)
     MLKEM_NAMESPACE=pqcrystals_mlkem768_ref_
else ifeq ($(MLKEM_K),4)
     MLKEM_NAMESPACE=pqcrystals_mlkem1024_ref_
else
     $(error Invalid value of MLKEM_K)
endif
